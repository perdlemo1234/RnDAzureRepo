{
	"name": "population_joincolumn",
	"properties": {
		"folder": {
			"name": "Basic Transformations"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "population_state",
						"type": "DatasetReference"
					},
					"name": "popstate"
				},
				{
					"linkedService": {
						"referenceName": "transformedblob",
						"type": "LinkedServiceReference"
					},
					"name": "popmsia"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "joincolumnoutput",
						"type": "DatasetReference"
					},
					"name": "simplejoinrnd"
				}
			],
			"transformations": [
				{
					"name": "jointest"
				}
			],
			"scriptLines": [
				"source(output(",
				"          state as string,",
				"          date as string,",
				"          sex as string,",
				"          age as string,",
				"          ethnicity as string,",
				"          population as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> popstate",
				"source(useSchema: false,",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delimited',",
				"     container: 'de-rnd',",
				"     folderPath: 'Input Folder ',",
				"     fileName: 'population_malaysia.csv',",
				"     columnDelimiter: ',',",
				"     escapeChar: '\\\\',",
				"     quoteChar: '\\\"',",
				"     columnNamesAsHeader: false) ~> popmsia",
				"popstate, popmsia join(popstate@population == ERROR_FUNCTION('')",
				"     && popstate@ethnicity == ERROR_FUNCTION(''),",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> jointest",
				"jointest sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          Year as string,",
				"          MoviesCount as string",
				"     ),",
				"     partitionFileNames:['filter_output '],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 1,",
				"     partitionBy('hash', 1)) ~> simplejoinrnd"
			]
		}
	}
}