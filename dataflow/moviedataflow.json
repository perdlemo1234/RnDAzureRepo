{
	"name": "moviedataflow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "moviescsv",
						"type": "DatasetReference"
					},
					"name": "movies"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "YearExtraction"
				}
			],
			"scriptLines": [
				"source(output(",
				"          movieId as string,",
				"          title as string,",
				"          genres as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> movies",
				"movies derive(Year = toInteger(trim(right(title,6),'()'))) ~> YearExtraction"
			]
		}
	}
}