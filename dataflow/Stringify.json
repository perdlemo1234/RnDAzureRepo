{
	"name": "Parsing",
	"properties": {
		"folder": {
			"name": "JSON transformations"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "student_json_input",
						"type": "DatasetReference"
					},
					"name": "Students"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "parse1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          id as integer,",
				"          name as string,",
				"          age as integer,",
				"          email as string,",
				"          skills as string[],",
				"          projects as (name as string, duration as string, role as string)[]",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     documentForm: 'arrayOfDocuments') ~> Students",
				"Students parse(age = ERROR_FUNCTION(''),",
				"     format: 'json',",
				"     documentForm: 'documentPerLine') ~> parse1"
			]
		}
	}
}